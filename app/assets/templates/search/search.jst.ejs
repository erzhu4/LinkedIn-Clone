<h2> Search Results </h2>
<div class="search-list">

  <% if (searches.length === 0) {%>
    No results to show.
  <%} else {%>

    <% searches.forEach( function (search){ %>
      <div class="group">
        <img src="<%=search.img_url%>" class="small-profile-picture">
        <%= search.fname + ' ' + search.lname %><br>
        <%= search.title %>
      </div>
    <%})%>
  <%}%>
</div>
